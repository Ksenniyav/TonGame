{
  "name": "webrtc-shooter",
  "version": "0.0.1",
  "description": "A short experiment on making peer to peer game with webrtc and impactjs",
  "scripts": {
    "sg-start": "pm2 start signalling/server.js -i 1 --name signalling",
    "sg-stop": "pm2 delete signalling",
    "signalling": "node signalling/server.js",
    "http": " http-server -c-1 . -p 8080",
    "start:dev:signalling": "nodemon signalling/server.js",
    "start:dev:server": "nodemon server.js"
  },
  "keywords": [
    "webrtc",
    "p2p",
    "games",
    "javascript"
  ],
  "author": "alx.gutnikov@gmail.com",
  "devDependencies": {
    "http-server": "^0.9.0",
    "nodemon": "^2.0.19",
    "socket.io": "^1.4.8"
  },
  "jscsConfig": {
    "preset": "google",
    "maxErrors": 100,
    "maximumLineLength": 100,
    "validateIndentation": 2
  },
  "dependencies": {
    "browserify": "^17.0.0",
    "buffer": "^6.0.3",
    "dotenv": "^16.0.2",
    "ethers": "^5.7.0",
    "express": "^4.18.1",
    "pg": "^8.8.0",
    "redis": "^4.3.1",
    "tonweb": "^0.0.50",
    "web3": "^1.7.5"
  }
}
